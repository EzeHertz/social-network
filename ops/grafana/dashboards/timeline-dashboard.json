{
  "title": "Timeline Fanout â€” Observability",
  "uid": "timeline-observability",
  "tags": ["timeline", "observability"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Tweets procesados",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        { "expr": "rate(timeline_tweet_events_total[1m])", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "type": "timeseries",
      "title": "Fanout Users vs Celebrities",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        { "expr": "rate(timeline_fanout_users_total[1m])", "legendFormat": "Users", "refId": "A" },
        { "expr": "rate(timeline_fanout_celebrities_total[1m])", "legendFormat": "Celebrities", "refId": "B" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "type": "timeseries",
      "title": "Fanout Latency p95",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(timeline_fanout_latency_seconds_bucket[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ],
  "time": { "from": "now-1h", "to": "now" }
}
